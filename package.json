{
  "name": "next-redux-rtk",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,mjs,cjs,json}'",
    "check-format": "prettier --check 'src/**/*.{js,jsx,ts,tsx,mjs,cjs,json}'",
    "check-lint": "eslint src -c .eslintrc.json --ext .js,.jsx,.ts,.tsx",
    "check-types": "npx tsc --pretty --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest --run --reporter=verbose",
    "postinstall": "prisma generate",
    "postbuild": "npx tsx src/lib/scripts/generate-sitemap.ts",
    "db-migrate": "npx prisma migrate dev",
    "db-studio": "npx prisma studio",
    "db-seed": "npx tsx ./src/lib/db/seed.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroui/react": "^2.7.8",
    "@hookform/resolvers": "^5.0.1",
    "@mdx-js/loader": "^3.1.0",
