    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^15.3.3",
    "@prisma/client": "^6.7.0",
    "@reduxjs/toolkit": "^2.8.1",
    "@tailwindcss/typography": "^0.5.16",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "clsx": "^2.1.1",
    "formidable": "^3.5.4",
    "framer-motion": "^12.11.3",
    "isomorphic-dompurify": "^2.25.0",
    "next": "^14.2.28",
    "next-auth": "^4.24.11",
    "next-redux-wrapper": "^8.1.0",
    "next-themes": "^0.4.6",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "rehype-prism-plus": "^2.0.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/ui": "^3.2.2",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^8.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^3.2.2"
  }
